{
  "name": "stable-credit-template",
  "version": "0.0.1",
  "main": "index.js",
  "scripts": {
    "chain": "yarn hardhat node --network hardhat ",
    "test": "forge test --fork-url https://mainnet.infura.io/v3/d3d4b9de93c64ea98a46306b336ef458",
    "coverage": "forge coverage --fork-url https://mainnet.infura.io/v3/d3d4b9de93c64ea98a46306b336ef458 --report lcov",
    "postcompile": "yarn typechain",
    "compile": "yarn hardhat compile",
    "typechain": "yarn hardhat typechain",
    "deploy": "yarn hardhat deploy --write true",
    "deploy-mock": "yarn hardhat deploy --tags MOCK",
    "demo": "yarn deploy-mock --network ganache && yarn hardhat demoSetup --network ganache",
    "watch": "yarn node scripts/watch.js",
    "accounts": "yarn hardhat accounts",
    "balance": "yarn hardhat balance",
    "send": "yarn hardhat send",
    "generate": "yarn hardhat generate",
    "account": "yarn hardhat account",
    "clean-local-deployments": "rm -rf deployments/localhost/*",
    "clean": "yarn hardhat clean"
  },
  "license": "MIT",
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.388.0",
